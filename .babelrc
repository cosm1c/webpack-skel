{
    "comments": false,
    "minified": true,
    "env": {
        "production": {
            "presets": [
                "minify"
            ]
        }
    },
    "presets": [
        "react",
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions",
                        "ie >= 11",
                        "> 1%"
                    ]
                },
                "modules": false
            }
        ]
    ],
    "plugins": [
        "transform-class-properties",
        "syntax-dynamic-import",
        "react-loadable/babel"
    ]
}
